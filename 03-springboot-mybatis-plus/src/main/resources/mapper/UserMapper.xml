<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hai.mapper.user.UserMapper">


    <select id="getUsers" resultType="com.hai.pojo.user.User">

        select id, nickname, password, age, male, user_intro  from kss_user where id=#{id};
    </select>
</mapper>
